{
  "name": "nest",
  "version": "1.0.0",
  "description": "videogame",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "server": "json-server data/table.json --watch",
    "build": "browserify ./client -o bundle.js -g uglifyify",
    "test": "node test/*.js | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/josephquested/nest"
  },
  "author": "josephquested",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/josephquested/nest/issues"
  },
  "homepage": "https://github.com/josephquested/nest",
  "dependencies": {},
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "express": "^4.13.4",
    "gh-pages": "^0.3.0",
    "jquery": "^2.2.0",
    "json-server": "^0.8.7",
    "react": "^0.14.7",
    "smokestack": "^3.3.0",
    "superagent": "^1.7.2",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  }
}
